    "workStart": {
      "type": "string"
    },
    "workEnd": {
      "type": "string"
    },
    "workPeriod": {
      "type": "string"
    },
    "breakStart": {
      "type": "string"
    },
    "breakEnd": {
      "type": "string"
    },
    "step": {
      "type": "number"
    },
    "reporter": {
      "type": "string",
